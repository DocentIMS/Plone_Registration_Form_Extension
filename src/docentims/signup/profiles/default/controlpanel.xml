<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="docentims.signup">

    <configlet
        title="Docent IMS Site Registration Settings"
        action_id="TypesSettings"
        appId="Plone"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@docentims-settings"
        icon_expr="string:$portal_url/document_icon.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>